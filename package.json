    "googleapis": "^144.0.0",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.8",
    "ms": "^3.0.0-canary.1",
    "nestjs-prisma": "^0.23.0",
    "nodemailer": "^6.9.15",
    "nodemon": "^3.1.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
